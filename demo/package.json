    "paliari-vue": "^0.0.60",
    "purecss": "^0.6.2",
    "string-mask": "^0.3.0",
    "vue-router": "^2.2.0",
    "vue-spinner": "^1.0.2",
    "vuex": "^2.1.2"
  },
  "peerDependencies": {
    "vwp": ">=0.0.32"
  }
}
