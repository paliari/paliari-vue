{
  "name": "paliari-vue",
  "version": "2.0.5",
  "main": "dist/paliari-vue.umd.min.js",
  "typings": "types/index.d.ts",
  "style": "dist/paliari-vue.css",
  "scripts": {
    "serve": "vue-cli-service serve ./demo/main.js",
    "build": "vue-cli-service build --target lib ./src/index.js",
