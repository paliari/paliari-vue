{
  "name": "paliari-vue",
  "version": "1.0.26",
  "description": "A Vue.js Plugin",
  "author": "Marcos Paliari <marcos@paliari.com.br>",
  "main": "dist/paliari-vue.min.js",
  "style": "dist/paliari-vue.min.css",
  "scripts": {
    "build": "bin/build",
    "dist": "bin/dist",
    "release": "rm -rf dist && yarn run build && yarn run dist",
    "dist:demo": "poi build ./demo/index.js --config ./demo/build.config.js --filename.css paliari-vue.css --no-sourceMap --dist ./docs",
    "start": "yarn dev",
    "dev": "poi build ./demo/index.js --config ./demo/build.config.js --filename.css paliari-vue.css --no-sourceMap --dist ./docs",
    "docs:open": "serve ./docs"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "es6-promise": "^4.2.4",
    "fecha": "^2.3.2",
    "string-mask": "^0.3.0",
    "vue": "^2.5.13",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "poi": "^9.6.13",
    "serve": "^7.0.0",
    "vwp": "^0.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paliari/paliari-vue.git"
  },
  "bugs": {
    "url": "https://github.com/paliari/paliari-vue/issues"
  },
  "homepage": "https://github.com/paliari/paliari-vue#readme",
  "license": {
    "type": "MIT",
    "url": "http://www.opensource.org/licenses/mit-license.php"
  }
}
