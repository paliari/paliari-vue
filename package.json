{
  "name": "paliari-vue",
  "version": "1.0.15",
  "description": "A Vue.js Plugin",
  "author": "Marcos Paliari <marcos@paliari.com.br>",
  "main": "dist/paliari-vue.js",
  "style": "dist/paliari-vue.css",
  "scripts": {
    "build": "bin/build",
    "dist": "bin/dist",
    "release": "rm -rf dist && yarn run build && yarn run dist",
    "dist:demo": "vue build ./demo/index.js --config ./demo/build.config.js --dist ./docs/ --prod",
    "start": "yarn dev",
    "dev": "vue build ./demo/index.js --config ./demo/build.config.js",
    "docs:open": "serve ./docs"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "fecha": "^2.3.1",
    "string-mask": "^0.3.0",
    "vue": "^2.3.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {},
  "peerDependencies": {
    "vwp": ">=0.0.33"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paliari/paliari-vue.git"
  },
  "bugs": {
    "url": "https://github.com/paliari/paliari-vue/issues"
  },
  "homepage": "https://github.com/paliari/paliari-vue#readme",
